{
  "version": 2,
  "name": "quips-rob-lindsey-com",
  "alias": [
    "quips.roblindsey.com"
  ],
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": {
        "distDir": "www"
      }
    }
  ],
  "routes": [
    { "src": "/", "dest": "/archive/0"},
    { "src": "/assets/images/global/.*", "headers": {"cache-control": "s-maxage=31536000"} },
    { "src": "/assets/fonts/.*", "headers": {"cache-control": "s-maxage=31536000"} }
  ]
}
